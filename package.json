{
  "name": "fastify-auth-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run build:back",
    "build:back": "tsc -p src/back/main/tsconfig.json",
    "lint": "tslint src/back/**/*.ts",
    "prettier": "prettier .",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "start": "per-env",
    "start:development": "nodemon --exec ts-node --esm src/back/main",
    "test": "echo 'NOT IMPLEMENTED'"
  },
  "description": "Simple authentication api with fastify",
  "dependencies": {
    "fastify": "^4.1.0",
    "per-env": "1.0.2",
    "source-map-support": "0.5.21"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.12.2"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "1.0.2",
    "@types/node": "18.0.0",
    "nodemon": "2.0.16",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "tslint": "6.1.3",
    "typescript": "4.7.4"
  },
  "type": "module"
}
