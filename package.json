{
  "name": "fastify-auth-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run build:back",
    "build:back": "tsc -p src/back/main/tsconfig.json",
    "build:src": "run-s build:src:check-types build:src:transpile",
    "build:src:check-types": "tsc --noemit -p ./src/tsconfig.json",
    "build:src:check-types:watch": "tsc --watch --noemit --preserveWatchOutput -p ./src/back/main/tsconfig.json",
    "build:src:transpile": "swc -s -d dist src/back/main src/test",
    "dev": "run-p build:src:check-types:watch dev:nodemon",
    "dev:nodemon": "npm run with:nodemon src/back/main/index.ts",
    "lint": "eslint src/back/**/*.ts",
    "prettier": "prettier .",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "start": "per-env",
    "start:development": "node -r @swc-node/register src/back/main",
    "test": "echo 'NOT IMPLEMENTED'",
    "with:nodemon": "npx -y nodemon@2.0.20"
  },
  "description": "Simple authentication api with fastify",
  "dependencies": {
    "dotenv": "16.0.3",
    "fastify": "4.14.1",
    "find-root": "1.1.0",
    "per-env": "1.0.2",
    "source-map-support": "0.5.21"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.12.2"
  },
  "devDependencies": {
    "@swc-node/register": "1.6.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.38",
    "@swc/jest": "0.2.24",
    "@tsconfig/node16-strictest-esm": "1.0.2",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "29.5.0",
    "jest-junit": "15.0.0",
    "jest-when": "3.5.2",
    "nodemon": "2.0.16",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "tslint": "6.1.3",
    "typescript": "4.7.4"
  }
}
